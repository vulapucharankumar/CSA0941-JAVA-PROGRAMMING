public class Outer {
    public void out()
    {
        System.out.println("outer clss method");
        
    }
    static class Inner
    {
        void in()
        {
            System.out.println("inner class method");
        }
        public static void main(String args[])
        {
            Outer o= new Outer();
            Inner i=new Inner();
            o.out();
            i.in();
        }
    }
    
}
